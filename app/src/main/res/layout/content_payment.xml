<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_payment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="edu.shsu.truck_it.PaymentActivity"
    tools:showIn="@layout/activity_payment">

    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:id="@+id/textView5"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Textview" />

    <TextView
        android:text="TextView"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:id="@+id/textView7"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="TextView"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:id="@+id/textView13"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="TextView"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:id="@+id/textView14"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:text="TextView"
        android:layout_height="30dp"
        android:id="@+id/textView15"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="200dp" />

    <TextView
        android:text="TextView"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/textView16"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:text="TextView"
        android:layout_height="30dp"
        android:id="@+id/textView18"
        android:layout_width="200dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:text="TextView"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/textView19"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView14"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
